{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@now/node"
    },
    {
      "src": "assets/**",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.+)",
      "dest": "assets/$1"
    }
  ]
}
